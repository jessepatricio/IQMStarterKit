@model IEnumerable<IQMStarterKit.Models.TempWorkbook>

@{
    ViewBag.Title = "Index";
}

<h2>Workbook List</h2>

@*<p>
    @Html.ActionLink("Create New", "Create")
</p>*@
<table class="table .table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Version)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Version)
        </td>
        
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.TempWorkbookId }) |
            @Html.ActionLink("Details", "Details", new { id=item.TempWorkbookId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.TempWorkbookId })
        </td>
    </tr>
}

</table>
