@model IEnumerable<IQMStarterKit.Models.TutorSurveyModel>

@using (Html.BeginForm())
{

    <br />
    <h2 class="heading">Tutor Survey List</h2>
    <table id="program" style="width:100%" class="table table-striped table-bordered" cellspacing="0">
        <thead>
            <tr>
                <th>Tutor Name</th>
                <th>Overall Rating</th>
                <th>Comment</th>
                <th>Submitted By</th>
                <th>Date Submitted</th>
               
               
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(m => item.TutorId)</td>
                    <td>@Html.DisplayFor(m => item.TOverall)</td>
                    <td>@Html.DisplayFor(m => item.TComment)</td>
                    <td>@Html.DisplayFor(m => item.CreatedBy)</td>
                    <td>@Html.DisplayFor(m => item.CreatedDateTime)</td>
                    

                    
                </tr>
            }
        </tbody>
    </table>



}



@section scripts{
    <script>

            $(document).ready(function () {
                $('#program').DataTable({
                    "bLengthChange": false,
                    "bFilter": true
                });
            });

           

    </script>
}