@section Scripts{

    <script type="text/javascript">

        function Validate() {
            for (var i = 1; i < 12; i++) {
                var radioValue = $("input[name='Leader" + i + "']:checked").val();
                if (radioValue == null) {
                    toastr.warning('not all rows were selected!');
                    return false;
                }
            }
            return true;
        }

        //confirmation dialog
        $('form').submit(function (e) {
            var currentForm = this;
            e.preventDefault();
            bootbox.confirm("Are you sure you want to submit your work?", function (result) {
                if (result) {
                    if (Validate()) {
                        currentForm.submit();
                    }
                }

            });
        });


    </script>
