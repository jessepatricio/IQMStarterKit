@model IQMStarterKit.Models.TOCViewModels
@{
    ViewBag.Title = "Index";
}
<link href="~/Content/main.css" rel="stylesheet" />
<br/>
<h3>IQM Modules (@ViewBag.StudentName)</h3>
@section Scripts {}


<div class="container1">
    @foreach (var item in Model.TempModules)
    {
        Random random = new Random();
        <button class="accordion">@Html.DisplayFor(modelItem => item.Title)</button>
        <div class="panel">
            @foreach (var act in item.TempActivities)
            {   
                //int randomNumber = random.Next(0, 100);

                <div class="row">
                    <div class="col-sm-5"><a class="activityLink" href="@Html.DisplayFor(modelItem => act.PageName)/@Html.DisplayFor(modelItem => act.TempActivityId)">@Html.DisplayFor(modelItem => act.Title)</a>
                    </div>
                    <div class=".visible-xs-block, hidden-xs col-sm-5">
                        @*<span class="progress-bar" role="progressbar" aria-valuenow="@randomNumber" aria-valuemin="0" aria-valuemax="100" style="width: @randomNumber%">@randomNumber%</span>*@
                        <span class="progress-bar" role="progressbar" aria-valuenow="@act.ProgressValue" aria-valuemin="0" aria-valuemax="100" style="width: @act.ProgressValue%">@act.ProgressValue%</span>
                    </div>
                   
                </div>
            }
        </div>
       
    }
    
   
</div>

<button onclick="topFunction()" id="myBtn" title="Go to top">Top</button>

<script src="~/Scripts/js/toc.js"></script>




